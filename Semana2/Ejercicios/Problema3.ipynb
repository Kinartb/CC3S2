   "source": [
    "def fib(n)\n",
    "  a, b = 0, 1\n",
    "  n.times do\n",
    "    yield b\n",
    "    a, b = b, a + b\n",
    "  end\n",
    "end\n",
    "\n",
    "fib(4) { |x| puts x }"
   ]
