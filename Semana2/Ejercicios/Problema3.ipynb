    "def odds(arr)\n",
    "  arr.each_with_index do |item, index|\n",
    "    yield item if index.odd?\n",
    "  end\n",
    "  nil\n",
    "end\n",
    "\n",
    "arr = [10, 30, 50, 70, 90]\n",
    "odds(arr) do |n|\n",
    "  puts n\n",
    "end\n",
    "\n"
   ]
